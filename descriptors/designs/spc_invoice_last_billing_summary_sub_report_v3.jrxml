			<subreport>
				<reportElement positionType="Float" x="59" y="157" width="232" height="21" isRemoveLineWhenBlank="true" uuid="6f4bb365-e480-45f8-8f48-0fbf50df5ec9"/>
				<subreportParameter name="SUBREPORT_DIR">
					<subreportParameterExpression><![CDATA[$P{SUBREPORT_DIR}]]></subreportParameterExpression>
				</subreportParameter>
				<subreportParameter name="other_charges_and_credits_product_category_id">
