	<subDataset name="dataset1">
		<parameter name="invoiceId" class="java.lang.Integer" isForPrompting="false"/>
		<queryString>
			<![CDATA[select uom.edi_file_field_value as uom, meter_id.edi_file_field_value as meter_id, start_read.edi_file_field_value as start_read
,end_read.edi_file_field_value as end_read, multiplier.edi_file_field_value as multiplier, total_consumption.edi_file_field_value as total_consumption
from invoice_meta_field_map rdimm
join meta_field_value rdval on rdval.id = rdimm.meta_field_value_id
join meta_field_name rdmf on rdval.meta_field_name_id = rdmf.id and lower(rdmf.name)='meter read file'
join edi_file f on f.id=rdval.integer_value
join edi_file_record umr on umr.edi_file_id=f.id and umr.edi_file_field_header='UMR' and umr.record_order >= 6
join edi_file_field uom on uom.edi_file_record_id=umr.id and uom.edi_file_field_key='UOM'
join edi_file_field meter_id on meter_id.edi_file_record_id=umr.id and meter_id.edi_file_field_key='METER_IDENTIFIER'
join edi_file_record qty on qty.edi_file_id=f.id and qty.edi_file_field_header='QTY' and qty.record_order=(umr.record_order+1)
join edi_file_field total_consumption on total_consumption.edi_file_record_id=qty.id and total_consumption.edi_file_field_key='TOTAL_CONSUMPTION'
join edi_file_record rea on rea.edi_file_id=f.id and rea.edi_file_field_header='REA' and rea.record_order=(umr.record_order+2)
join edi_file_field start_read on start_read.edi_file_record_id=rea.id and start_read.edi_file_field_key='START_METER_READ'
join edi_file_field end_read on end_read.edi_file_record_id=rea.id and end_read.edi_file_field_key='END_METER_READ'
join edi_file_field multiplier on multiplier.edi_file_record_id=rea.id and multiplier.edi_file_field_key='MULTIPLIER'
where rdimm.invoice_id = $P{invoiceId}]]>
		</queryString>
