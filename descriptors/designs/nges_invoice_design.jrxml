	<queryString>
		<![CDATA[(select il.id, 1 as type, il.amount, il.quantity, il.price, uom.uom, (case when id.content is null then il.description else id.content end) as line_desc
from invoice inv
join invoice_line il on il.invoice_id=inv.id and inv.id=$P{invoiceId}  and il.deleted=0 and il.type_id != 2
join item it on it.id=il.item_id
left join (
  select uomit.id, uomval.string_value as uom
  from item uomit
  join item_meta_field_map uomimm on uomimm.item_id=uomit.id
    join meta_field_value uomval on uomval.id = uomimm.meta_field_value_id
    join meta_field_name uommf on uomval.meta_field_name_id = uommf.id and lower(uommf.name)='unit of measure'
) as uom on uom.id=it.id
left join plan_item pi on pi.item_id=it.id
left join customer_price cp on cp.plan_item_id=pi.id and
  cp.create_datetime = (select max(create_datetime) from customer_price where plan_item_id=pi.id and user_id=inv.user_id)
left join international_description id on id.foreign_id=pi.plan_id and id.table_id=14 and id.language_id=1 and id.psudo_column='description')
union
(select (case when il.parent_line_id is null then il.id else il.parent_line_id end) as id, 2 as type, il.amount, il.quantity, il.price, '' as uom, il.description as line_desc
from invoice inv
join invoice_line il on il.invoice_id=inv.id and inv.id=$P{invoiceId} and il.deleted=0 and il.type_id = 2
)
order by id, type, line_desc]]>
	</queryString>
