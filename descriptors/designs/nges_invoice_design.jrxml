		<field name="meter_id" class="java.lang.String"/>
		<field name="start_read" class="java.lang.String"/>
		<field name="end_read" class="java.lang.String"/>
		<field name="multiplier" class="java.lang.String"/>
		<field name="total_consumption" class="java.lang.String"/>
	</subDataset>
