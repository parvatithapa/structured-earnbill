                            JOIN   meta_field_name mfn
                            ON     mfn.id = mfv.meta_field_name_id
                            WHERE  e.id = u.entity_id
                            AND    mfn.NAME = 'Customer Contact AIT Name' ) ) AS to_state,
       (
              SELECT String_agg(mfv.string_value, ', ') AS address
