              WHERE  e.id = u.entity_id
              AND    mfn.NAME = 'Bank Name' ) AS bank_name,
       (
              SELECT mfv.string_value
              FROM   entity e
              JOIN   entity_meta_field_map emfm
              ON     e.id = emfm.entity_id
