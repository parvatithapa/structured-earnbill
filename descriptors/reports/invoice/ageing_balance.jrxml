	<parameter name="entity_id" class="java.lang.Integer"/>
	<parameter name="child_entities" class="java.util.List"/>
	<queryString>
		<![CDATA[    SELECT u.id AS user_id,
            		        u.user_name,
            			    first_name_value.string_value AS first_name,
            			    last_name_value.string_value AS last_name,
            				organization_name_value.string_value AS organization_name,
            				state_province_name_value.string_value AS state_province,
            				country_code_name_value.string_value AS country_code,
            				cu.auto_payment_type, -- convert to CH = Check, CC = credit card, ACH or blank
            				(SELECT int_des.content
            			   	   FROM international_description AS int_des
            			  	  WHERE table_id = (SELECT id
            			    				  	  FROM jbilling_table
            			    				 	 WHERE name = 'user_status')
             			    	AND foreign_id IN (SELECT status_id
             			      					 	 FROM base_user
													WHERE id = u.id)
								AND language_id =1) AS content, -- convert to A = Active, S = Suspended, O = Overdue
            				MAX(i.create_datetime) AS last_billing_date,
            				MAX(p.payment_date) AS last_payment_date,
            				((SELECT COALESCE(SUM(i.balance), 0)
            			    	FROM invoice AS i
            			   	   WHERE i.deleted = 0
            			     	 AND i.is_review != 1
            			     	 AND i.user_id = u.id
            			     	 AND i.currency_id = c.id) -
                		 	 (SELECT COALESCE(SUM(p.balance), 0)
                		    	FROM payment AS p
                		   	   WHERE p.deleted = 0
                                 AND p.is_refund = 0
                                 AND p.result_id in (1,4)
                		   	 	 AND p.user_id = u.id
								 AND p.currency_id = c.id)) AS current_balance,
                		   	c.symbol AS currency_symbol,
                		   	c.id AS currency_id
        		       FROM base_user AS u
                 INNER JOIN customer cu ON cu.user_id = u.id
            	  LEFT JOIN payment p ON p.user_id = u.id AND p.deleted = 0
            	  LEFT JOIN invoice i ON i.user_id = u.id AND i.deleted = 0 AND i.is_review = 0
            	  LEFT JOIN (	SELECT mfv.id, mfv.string_value, caitt.customer_id
								  FROM customer_account_info_type_timeline caitt
      						 LEFT JOIN meta_field_value mfv ON mfv.id = caitt.meta_field_value_id
      						 LEFT JOIN meta_field_name mfn ON mfv.meta_field_name_id = mfn.id
      							 WHERE mfn.field_usage = 'FIRST_NAME') AS first_name_value ON cu.id = first_name_value.customer_id
				  LEFT JOIN (	SELECT mfv.id, mfv.string_value, caitt.customer_id
								  FROM customer_account_info_type_timeline caitt
      						 LEFT JOIN meta_field_value mfv ON mfv.id = caitt.meta_field_value_id
      						 LEFT JOIN meta_field_name mfn ON mfv.meta_field_name_id = mfn.id
      							 WHERE mfn.field_usage = 'LAST_NAME') AS last_name_value ON cu.id = last_name_value.customer_id
            	  LEFT JOIN (	SELECT mfv.id, mfv.string_value, caitt.customer_id
								  FROM customer_account_info_type_timeline caitt
      						 LEFT JOIN meta_field_value mfv ON mfv.id = caitt.meta_field_value_id
      						 LEFT JOIN meta_field_name mfn ON mfv.meta_field_name_id = mfn.id
      							 WHERE mfn.field_usage = 'ORGANIZATION') AS organization_name_value ON cu.id = organization_name_value.customer_id
				  LEFT JOIN (	SELECT mfv.id, mfv.string_value, caitt.customer_id
								  FROM customer_account_info_type_timeline caitt
      						 LEFT JOIN meta_field_value mfv ON mfv.id = caitt.meta_field_value_id
      						 LEFT JOIN meta_field_name mfn ON mfv.meta_field_name_id = mfn.id
      							 WHERE mfn.field_usage = 'STATE_PROVINCE') AS state_province_name_value ON cu.id = state_province_name_value.customer_id
				  LEFT JOIN (	SELECT mfv.id, mfv.string_value, caitt.customer_id
								  FROM customer_account_info_type_timeline caitt
      						 LEFT JOIN meta_field_value mfv ON mfv.id = caitt.meta_field_value_id
      						 LEFT JOIN meta_field_name mfn ON mfv.meta_field_name_id = mfn.id
      							 WHERE mfn.field_usage = 'COUNTRY_CODE') AS country_code_name_value ON cu.id = country_code_name_value.customer_id
				 INNER JOIN currency c ON c.id = p.currency_id OR c.id = i.currency_id
        			  WHERE (u.entity_id = $P{entity_id} OR $X{IN,u.entity_id,child_entities})
            		    AND u.deleted = 0
        		   GROUP BY u.id,
        		    		u.user_name,
        		    		first_name_value.string_value,
            				last_name_value.string_value,
            				organization_name_value.string_value,
            				state_province_name_value.string_value,
            				country_code_name_value.string_value,
            				cu.auto_payment_type,
            				u.status_id,
            				c.id,
            				currency_symbol
					 HAVING ((SELECT COALESCE(SUM(i.balance), 0)
         						FROM invoice AS i
							   WHERE i.deleted = 0
               					 AND i.is_review != 1
               					 AND i.user_id = u.id
               					 AND i.currency_id = c.id) -
        					 (SELECT COALESCE(SUM(p.balance), 0)
         						FROM payment AS p
							   WHERE p.deleted = 0
                                 AND p.is_refund = 0
                                 AND p.result_id in (1,4)
								 AND p.user_id = u.id
								 AND p.currency_id = c.id)) != 0
				    ORDER BY u.user_name,
				    		 c.id]]>
	</queryString>
